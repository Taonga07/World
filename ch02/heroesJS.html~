<h1>Heroes in JavaScript</h1> 

<script language = "javascript">
heroes = array("Buffalo Man", 
               "Geek Boy",
               "Wiffle-Ball Woman");

foreach (hero in heroes){
  document.writeline("Never fear, " + hero + " is here!";
} 








</script>
